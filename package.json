{
  "name": "vega-editor",
  "version": "1.1.1",
  "description": "Web-based Vega authoring environment.",
  "keywords": [
    "vega",
    "editor",
    "JSON",
    "visualization",
    "interaction",
    "dataflow",
    "grammar",
    "d3"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/vega/vega-editor.git"
  },
  "author": {
    "name": "Jeffrey Heer",
    "url": "http://idl.cs.washington.edu"
  },
  "contributors": [
    {"name": "Ruijia (Iris) Wang"}
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "ace-builds": "ajaxorg/ace-builds",
    "d3": "^3.5.6",
    "d3-cloud": "~1.1.0",
    "d3-geo-projection": "^0.2.15",
    "topojson": "^1.6.19",
    "vega": "latest",
    "vega-embed": "latest"
  },
  "devDependencies": {
    "chokidar": "^1.0.3"
  },
  "scripts": {
    "data": "git subtree pull --prefix app/data git@github.com:vega/vega-datasets.git gh-pages",
    "deploy": "scripts/deploy.sh",
    "vendor": "npm install && scripts/vendor.sh",
    "postvendor": "npm run data",
    "watch": "node scripts/watch.js"
  }
}
